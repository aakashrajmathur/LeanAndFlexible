        <Label x:Name="prioritySliderLabel" Content="Label" HorizontalAlignment="Left" Margin="701,289,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Margin="10,443,0,11" Width="170"/>
        <ListBox x:Name="listBox1" HorizontalAlignment="Left" Margin="185,443,0,11" Width="190"/>
        <ListBox x:Name="listBox2" HorizontalAlignment="Left" Margin="380,443,0,11" Width="191"/>
        <ListBox x:Name="listBox3" HorizontalAlignment="Left" Margin="576,443,0,11" Width="190"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="656,85,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="576,85,0,0"/>
        <Button x:Name="button2" Content="Button" HorizontalAlignment="Left" Margin="496,85,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="statusLabel" Content="Label" HorizontalAlignment="Left" Margin="10,355,0,0" VerticalAlignment="Top" Width="756" Height="83"/>

    </Grid>
</UserControl>
