using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace FlowAssessmentTool
{
    public class Machine : IComparable<Machine>
    {
        public string name;

        public Machine(string name)
        {
            this.name = name;
        }

        public int CompareTo(Machine anotherMachine)
        {
            if (this.name.CompareTo(anotherMachine.name) == 0)
            {
                return 0;
            }
            else
            {
                return -1;
            }
        }

        public string ToJSONString()
        {
            //Panding...
            return "";
        }

        public string ToXML()
        {
            string temp = "<Machine>" + Environment.NewLine;
            temp += "<Name>" + name + "</Name>" + Environment.NewLine;
            temp += "</Machine>" + Environment.NewLine;
            return temp;
        }
    }
}