<UserControl x:Class="NewHorizons.PriorityUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:wpfTool="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
             xmlns:local="clr-namespace:NewHorizons"
             mc:Ignorable="d" Width="775.573" 
             IsVisibleChanged="UserControl_IsVisibleChanged" MinWidth="400" MaxWidth="800" Height="553.71">

    <Grid>

        <Label FontSize="36" Foreground="White" FontFamily="Microsoft YaHei UI Light" Height="59" VerticalAlignment="Top">Welcome to Priority!</Label>
        <Slider x:Name="PrioritySlider" HorizontalAlignment="Left" Margin="50,294,0,0" VerticalAlignment="Top" Width="620" Height="21" ValueChanged="slider_ValueChanged" SmallChange="1" Maximum="100"/>
        <Label x:Name="prioritySliderLabel" Content="Label" HorizontalAlignment="Left" Margin="701,289,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
