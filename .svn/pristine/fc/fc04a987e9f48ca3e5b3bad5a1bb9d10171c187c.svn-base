<Window x:Class="FlowAssessmentTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlowAssessmentTool"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Menu x:Name="menu" Height="20" VerticalAlignment="Top">
            <MenuItem x:Name="FileMenuItem" Header="File"/>
            <MenuItem x:Name="DataTransferMenuItem" Header="Data Transfer">
                <MenuItem x:Name="ImportFromCSVFile" Header="Import From CSV File" HorizontalAlignment="Left" Click="ImportFromCSVFile_Click"/>
            </MenuItem>
            <MenuItem x:Name="ConfigMenuItem" Header="Configuration"/>
            <MenuItem x:Name="HelpMenuItem" Header="Help"/>
        </Menu>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Margin="22,53,0,35" Width="124"/>
        <ListBox x:Name="listBox1" Margin="151,53,204,35"/>
        <ListBox x:Name="listBox2" Margin="0,53,10,35" HorizontalAlignment="Right" Width="189"/>
        <Button x:Name="RefreshButton" Content="Refresh" Margin="0,0,10,10" Click="button_Click" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
        <Button x:Name="PriorityButton" Content="Priority" Margin="0,0,90,10" Click="PriorityButton_Click" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>

    </Grid>
</Window>
