             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NewHorizons"
             mc:Ignorable="d" Loaded="UserControl_Loaded" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" Width="800" MinWidth="400" MaxWidth="800" ScrollViewer.CanContentScroll="True" ClipToBounds="True" VerticalAlignment="Top" VerticalContentAlignment="Top">
    <Grid ClipToBounds="True" VerticalAlignment="Top">
        <Label FontSize="36" Foreground="White" FontFamily="Microsoft YaHei UI Light">Welcome Home</Label>
        <Label FontSize="24" Foreground="White" FontFamily="Microsoft YaHei UI Light" Margin="10,59,0,195" HorizontalAlignment="Left" Width="200">Selections</Label>
        <ListBox x:Name="selectionsListBox" HorizontalAlignment="Left" Margin="10,95,0,10" Width="200" Background="{x:Null}" VerticalContentAlignment="Stretch" ClipToBounds="True"/>

    </Grid>
</UserControl>
