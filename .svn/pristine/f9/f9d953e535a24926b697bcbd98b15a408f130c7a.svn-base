<UserControl x:Class="FlowAssessmentTool.PriorityUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FlowAssessmentTool"
             mc:Ignorable="d" 
             d:DesignHeight="600" Background="White" Width="647.5">
    <Grid Background="#FFEDF5E9" Loaded="Grid_Loaded">
        <Label x:Name="PriorityHeadingLabel" Content="PRIORITY" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.105,-0.462" FontSize="24" Foreground="#FF131223"/>
        <Canvas x:Name="priorityCanvas" HorizontalAlignment="Left" Height="298" Margin="320,76,0,0" VerticalAlignment="Top" Width="300" Background="#FFE2F5F4"/>
        <Slider x:Name="revenueSlider" HorizontalAlignment="Left" Margin="320,379,0,0" VerticalAlignment="Top" Height="20" Width="300" Maximum="100" SmallChange="1" ValueChanged="revenueSlider_ValueChanged"/>
        <Slider x:Name="quantitySlider" HorizontalAlignment="Left" Margin="291,76,0,0" VerticalAlignment="Top" Orientation="Vertical" Height="298" Width="24" ValueChanged="quantitySlider_ValueChanged" SmallChange="1" Maximum="100"/>
        <Label x:Name="label" Content="Revenue" HorizontalAlignment="Left" Margin="320,404,0,0" VerticalAlignment="Top" Width="141" HorizontalContentAlignment="Right"/>
        <Label x:Name="revenueSliderValueLabel" Content="Label" HorizontalAlignment="Left" Margin="466,404,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Quantity" HorizontalAlignment="Left" Margin="370,435,0,0" VerticalAlignment="Top" Width="91" HorizontalContentAlignment="Right"/>
        <Label x:Name="quantitySliderLabel" Content="Label" HorizontalAlignment="Left" Margin="466,435,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Margin="28,100,0,10" Width="236"/>
        <StackPanel HorizontalAlignment="Left" Height="74" Margin="320,466,0,0" VerticalAlignment="Top" Width="300" Orientation="Horizontal">
            <Label x:Name="label2" Content="" FontSize="28"/>
            <Label x:Name="label3" Content="Parts analyzed" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        </StackPanel>
        <Button x:Name="saveSelectionButton" Content="Button" Margin="0,0,28,10" HorizontalAlignment="Right" Width="75" Height="20" VerticalAlignment="Bottom"/>
        <Label x:Name="labelPartsIncluded" Content="Parts Included" HorizontalAlignment="Left" Margin="28,76,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
